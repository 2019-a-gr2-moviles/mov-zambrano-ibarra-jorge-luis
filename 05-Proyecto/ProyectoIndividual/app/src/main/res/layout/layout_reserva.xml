<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="450dp" android:orientation="vertical"
              android:background="@color/colorTextoBotones" android:id="@+id/linear_layout_res">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:background="@color/colorTextoBotones"
            android:id="@+id/lay_cam_res">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:background="@color/colorTextoBotones">
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="209dp"
                    android:layout_height="match_parent" android:background="@color/colorTextoBotones">
                <TextView
                        android:text="Id "
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/textView35" android:layout_weight="1"
                        android:textColor="@color/colorCampos" android:textStyle="bold" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:text="Fecha"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/textView34" android:layout_weight="1"
                        android:textColor="@color/colorCampos" android:textStyle="bold" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:text="Cliente:"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/textView33" android:layout_weight="1"
                        android:textColor="@color/colorCampos" android:textStyle="bold" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:text="Cancha"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/textView32" android:layout_weight="1"
                        android:textColor="@color/colorCampos" android:textStyle="bold" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:text="Hora I"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/textView18" android:layout_weight="1"
                        android:textColor="@color/colorCampos" android:textStyle="bold" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>

                <TextView
                        android:text="Hora F"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/textView19" android:layout_weight="1"
                        android:textColor="@color/colorCampos" android:textStyle="bold" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:text="Horas"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/textView31" android:layout_weight="1"
                        android:textColor="@color/colorCampos" android:textStyle="bold" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:text="Precio Unitario"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/textView37" android:layout_weight="1"
                        android:textSize="20sp" android:textColor="@color/colorCampos" android:textStyle="bold"/>
                <TextView
                        android:text="Total"
                        android:layout_width="match_parent"
                        android:layout_height="30dp" android:id="@+id/textView23" android:layout_weight="1"
                        android:textSize="20sp" android:textColor="@color/colorCampos" android:textStyle="bold"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" android:id="@+id/txt_id_res" android:layout_weight="1"
                        android:textColor="@color/colorTextoCampos" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" android:id="@+id/txt_fech_res" android:layout_weight="1"
                        android:textColor="@color/colorTextoCampos" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" android:id="@+id/txt_cli_res" android:layout_weight="1"
                        android:textColor="@color/colorTextoCampos" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" android:id="@+id/txt_can_res" android:layout_weight="1"
                        android:textColor="@color/colorTextoCampos" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/txt_horai_res" android:layout_weight="1"
                        android:textColor="@color/colorTextoCampos" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>


                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/txt_horaf_res" android:layout_weight="1"
                        android:textColor="@color/colorTextoCampos" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" android:id="@+id/txt_hor_res" android:layout_weight="1"
                        android:textColor="@color/colorTextoCampos" android:textSize="20sp"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/txt_pre_res" android:layout_weight="1"
                        android:textStyle="bold" android:textSize="20sp" android:textColor="@color/colorTextoCampos"
                        android:background="@color/colorTextoBotones"/>
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:id="@+id/txt_tot_com" android:layout_weight="1"
                        android:textStyle="bold" android:textSize="20sp" android:textColor="@color/colorTextoCampos"
                        android:background="@color/colorTextoBotones"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="85dp" android:background="@color/colorTextoBotones">

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView38" android:layout_weight="1"
                android:background="@color/colorTextoBotones"/>
        <Button
                android:text="@string/btn_eliminar"
                android:layout_width="102dp"
                android:layout_height="wrap_content" android:id="@+id/btn_anu_com" android:layout_weight="1"
                android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
                android:textColor="@color/colorTextoBotones" android:background="@color/colorBotones"
                android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                android:visibility="visible"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView39" android:layout_weight="1"
                android:background="@color/colorTextoBotones"/>
    </LinearLayout>
</LinearLayout>
</LinearLayout>