<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.proyectoindividual">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true">
        <activity android:name="com.example.proyectoindividual.modulo_canchas.CrearCancha">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_reservas.CrearReserva">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_reservas.ActualizarReserva">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_reservas.ListaReserva">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_canchas.ActualizarCancha">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_clientes.ActualizarCliente">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_clientes.CrearCliente">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_canchas.ListaCancha">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_clientes.ListaClientes">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_reservas.MenuReservas">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_clientes.MenuClientes">
        </activity>
        <activity android:name="com.example.proyectoindividual.modulo_canchas.MenuCancha">
        </activity>
        <activity android:name="com.example.proyectoindividual.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>